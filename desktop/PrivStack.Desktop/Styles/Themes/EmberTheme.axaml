<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- PrivStack Ember Theme (Dark) -->
    <!-- Warm earthy tones with olive and burnt orange accents -->

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- BACKGROUND COLORS                                                        -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeBackground">#0E0E0A</Color>
    <Color x:Key="ThemeSurface">#1A1C14</Color>
    <Color x:Key="ThemeSurfaceElevated">#262820</Color>
    <Color x:Key="ThemeSurfaceGlass">#D91A1C14</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- BORDER COLORS                                                            -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeBorder">#3A3C30</Color>
    <Color x:Key="ThemeBorderSubtle">#2C2E24</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- TEXT COLORS                                                              -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeTextPrimary">#F0EAE0</Color>
    <Color x:Key="ThemeTextSecondary">#BEB8A8</Color>
    <Color x:Key="ThemeTextMuted">#908A78</Color>
    <Color x:Key="ThemeTextOnAccent">#FFFFFF</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- PRIMARY ACCENT (BURNT ORANGE)                                            -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemePrimary">#D08040</Color>
    <Color x:Key="ThemePrimaryHover">#C07030</Color>
    <Color x:Key="ThemePrimaryMuted">#66D08040</Color>
    <Color x:Key="ThemePrimaryGlow">#80D08040</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- SECONDARY ACCENT (OLIVE GREEN)                                           -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeSecondary">#7A9860</Color>
    <Color x:Key="ThemeSecondaryHover">#6A8850</Color>
    <Color x:Key="ThemeSecondaryMuted">#667A9860</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- SEMANTIC COLORS                                                          -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeSuccess">#5A9848</Color>
    <Color x:Key="ThemeSuccessMuted">#4D5A9848</Color>
    <Color x:Key="ThemeWarning">#D0A030</Color>
    <Color x:Key="ThemeWarningMuted">#4DD0A030</Color>
    <Color x:Key="ThemeDanger">#D04040</Color>
    <Color x:Key="ThemeDangerMuted">#4DD04040</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- INTERACTION STATES                                                       -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeHover">#2A2C22</Color>
    <Color x:Key="ThemeSelected">#2E2820</Color>
    <Color x:Key="ThemePressed">#343630</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- SPECIAL COLORS                                                           -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <Color x:Key="ThemeSelection">#4DD08040</Color>
    <Color x:Key="ThemeScrollTrack">#0E0E0A</Color>
    <Color x:Key="ThemeScrollThumb">#404238</Color>
    <Color x:Key="ThemeScrollThumbHover">#5A5C50</Color>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- BRUSH RESOURCES                                                          -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <SolidColorBrush x:Key="ThemeBackgroundBrush" Color="{StaticResource ThemeBackground}"/>
    <SolidColorBrush x:Key="ThemeSurfaceBrush" Color="{StaticResource ThemeSurface}"/>
    <SolidColorBrush x:Key="ThemeSurfaceElevatedBrush" Color="{StaticResource ThemeSurfaceElevated}"/>
    <SolidColorBrush x:Key="ThemeSurfaceGlassBrush" Color="{StaticResource ThemeSurfaceGlass}"/>

    <SolidColorBrush x:Key="ThemeBorderBrush" Color="{StaticResource ThemeBorder}"/>
    <SolidColorBrush x:Key="ThemeBorderSubtleBrush" Color="{StaticResource ThemeBorderSubtle}"/>

    <SolidColorBrush x:Key="ThemeTextPrimaryBrush" Color="{StaticResource ThemeTextPrimary}"/>
    <SolidColorBrush x:Key="ThemeTextSecondaryBrush" Color="{StaticResource ThemeTextSecondary}"/>
    <SolidColorBrush x:Key="ThemeTextMutedBrush" Color="{StaticResource ThemeTextMuted}"/>
    <SolidColorBrush x:Key="ThemeTextOnAccentBrush" Color="{StaticResource ThemeTextOnAccent}"/>

    <SolidColorBrush x:Key="ThemePrimaryBrush" Color="{StaticResource ThemePrimary}"/>
    <SolidColorBrush x:Key="ThemeLinkBrush" Color="{StaticResource ThemePrimary}"/>
    <SolidColorBrush x:Key="ThemePrimaryHoverBrush" Color="{StaticResource ThemePrimaryHover}"/>
    <SolidColorBrush x:Key="ThemePrimaryMutedBrush" Color="{StaticResource ThemePrimaryMuted}"/>

    <SolidColorBrush x:Key="ThemeSecondaryBrush" Color="{StaticResource ThemeSecondary}"/>
    <SolidColorBrush x:Key="ThemeSecondaryHoverBrush" Color="{StaticResource ThemeSecondaryHover}"/>
    <SolidColorBrush x:Key="ThemeSecondaryMutedBrush" Color="{StaticResource ThemeSecondaryMuted}"/>

    <SolidColorBrush x:Key="ThemeSuccessBrush" Color="{StaticResource ThemeSuccess}"/>
    <SolidColorBrush x:Key="ThemeSuccessMutedBrush" Color="{StaticResource ThemeSuccessMuted}"/>
    <SolidColorBrush x:Key="ThemeWarningBrush" Color="{StaticResource ThemeWarning}"/>
    <SolidColorBrush x:Key="ThemeWarningMutedBrush" Color="{StaticResource ThemeWarningMuted}"/>
    <SolidColorBrush x:Key="ThemeDangerBrush" Color="{StaticResource ThemeDanger}"/>
    <SolidColorBrush x:Key="ThemeDangerMutedBrush" Color="{StaticResource ThemeDangerMuted}"/>

    <SolidColorBrush x:Key="ThemeHoverBrush" Color="{StaticResource ThemeHover}"/>
    <SolidColorBrush x:Key="ThemeSelectedBrush" Color="{StaticResource ThemeSelected}"/>
    <SolidColorBrush x:Key="ThemePressedBrush" Color="{StaticResource ThemePressed}"/>

    <!-- Navigation Sidebar Brushes (dark theme — sidebar matches content) -->
    <SolidColorBrush x:Key="ThemeNavBackgroundBrush" Color="{StaticResource ThemeSurface}"/>
    <SolidColorBrush x:Key="ThemeNavBorderBrush" Color="{StaticResource ThemeBorder}"/>
    <SolidColorBrush x:Key="ThemeNavBorderSubtleBrush" Color="{StaticResource ThemeBorderSubtle}"/>
    <SolidColorBrush x:Key="ThemeNavTextBrush" Color="{StaticResource ThemeTextMuted}"/>
    <SolidColorBrush x:Key="ThemeNavTextHoverBrush" Color="{StaticResource ThemeTextPrimary}"/>
    <SolidColorBrush x:Key="ThemeNavHoverBrush" Color="{StaticResource ThemeHover}"/>
    <SolidColorBrush x:Key="ThemeNavSelectedBrush" Color="{StaticResource ThemeSelected}"/>

    <SolidColorBrush x:Key="ThemeTableHeaderBrush" Color="{StaticResource ThemeHover}"/>
    <SolidColorBrush x:Key="ThemeTableStripeBrush" Color="{StaticResource ThemeHover}"/>
    <SolidColorBrush x:Key="ThemeTableSeparatorBrush" Color="{StaticResource ThemeBorderSubtle}"/>

    <!-- Table Color Theme Presets -->
    <SolidColorBrush x:Key="ThemeTableBlueHeaderBrush" Color="#1A3B5C8A"/>
    <SolidColorBrush x:Key="ThemeTableBlueStripeBrush" Color="#0D3B5C8A"/>
    <SolidColorBrush x:Key="ThemeTableGreenHeaderBrush" Color="#1A4CAF50"/>
    <SolidColorBrush x:Key="ThemeTableGreenStripeBrush" Color="#0D4CAF50"/>
    <SolidColorBrush x:Key="ThemeTablePurpleHeaderBrush" Color="#1A7B1FA2"/>
    <SolidColorBrush x:Key="ThemeTablePurpleStripeBrush" Color="#0D7B1FA2"/>
    <SolidColorBrush x:Key="ThemeTableOrangeHeaderBrush" Color="#1AFF9800"/>
    <SolidColorBrush x:Key="ThemeTableOrangeStripeBrush" Color="#0DFF9800"/>

    <!-- Code Block Brushes -->
    <SolidColorBrush x:Key="ThemeCodeBlockBrush" Color="{StaticResource ThemeBackground}"/>
    <SolidColorBrush x:Key="ThemeCodeBlockGutterBrush" Color="{StaticResource ThemeSurface}"/>

    <SolidColorBrush x:Key="ThemeSelectionBrush" Color="{StaticResource ThemeSelection}"/>
    <SolidColorBrush x:Key="ThemeScrollTrackBrush" Color="{StaticResource ThemeScrollTrack}"/>
    <SolidColorBrush x:Key="ThemeScrollThumbBrush" Color="{StaticResource ThemeScrollThumb}"/>
    <SolidColorBrush x:Key="ThemeScrollThumbHoverBrush" Color="{StaticResource ThemeScrollThumbHover}"/>

    <!-- ═══════════════════════════════════════════════════════════════════════ -->
    <!-- LAYOUT CONSTANTS                                                         -->
    <!-- ═══════════════════════════════════════════════════════════════════════ -->

    <CornerRadius x:Key="ThemeRadiusXs">4</CornerRadius>
    <CornerRadius x:Key="ThemeRadiusSm">6</CornerRadius>
    <CornerRadius x:Key="ThemeRadiusMd">8</CornerRadius>
    <CornerRadius x:Key="ThemeRadiusLg">12</CornerRadius>
    <CornerRadius x:Key="ThemeRadiusXl">16</CornerRadius>
    <CornerRadius x:Key="ThemeRadiusFull">9999</CornerRadius>

    <Thickness x:Key="ThemePaddingXs">4</Thickness>
    <Thickness x:Key="ThemePaddingSm">8</Thickness>
    <Thickness x:Key="ThemePaddingMd">12</Thickness>
    <Thickness x:Key="ThemePaddingLg">16</Thickness>
    <Thickness x:Key="ThemePaddingXl">24</Thickness>
    <Thickness x:Key="ThemePadding2Xl">32</Thickness>

    <x:Double x:Key="ThemeSidebarWidth">260</x:Double>
    <x:Double x:Key="ThemeDetailPanelWidth">320</x:Double>
    <x:Double x:Key="ThemeInfoPanelWidth">280</x:Double>
    <x:Double x:Key="ThemePopupPanelWidth">320</x:Double>
    <x:Double x:Key="ThemeSidebarNarrowWidth">200</x:Double>

    <Thickness x:Key="ThemeListItemPadding">12,10</Thickness>
    <Thickness x:Key="ThemeListItemPaddingCompact">8,6</Thickness>
    <x:Double x:Key="ThemeListItemMinHeight">44</x:Double>
    <x:Double x:Key="ThemeListItemMinHeightCompact">36</x:Double>
    <x:Double x:Key="ThemeListItemSpacing">2</x:Double>

    <x:Double x:Key="ThemeAvatarSizeSm">32</x:Double>
    <x:Double x:Key="ThemeAvatarSizeMd">40</x:Double>
    <x:Double x:Key="ThemeAvatarSizeLg">64</x:Double>
    <x:Double x:Key="ThemeAvatarSizeXl">80</x:Double>
    <x:Double x:Key="ThemeIconSizeXs">12</x:Double>
    <x:Double x:Key="ThemeIconSizeSm">16</x:Double>
    <x:Double x:Key="ThemeIconSizeMd">20</x:Double>
    <x:Double x:Key="ThemeIconSizeLg">24</x:Double>
    <x:Double x:Key="ThemeIconSizeXl">32</x:Double>
    <x:Double x:Key="ThemeStatusDotSize">8</x:Double>

    <x:Double x:Key="ThemeSpacingXs">4</x:Double>
    <x:Double x:Key="ThemeSpacingSm">8</x:Double>
    <x:Double x:Key="ThemeSpacingMd">12</x:Double>
    <x:Double x:Key="ThemeSpacingLg">16</x:Double>
    <x:Double x:Key="ThemeSpacingXl">24</x:Double>
    <x:Double x:Key="ThemeSpacing2Xl">32</x:Double>
    <x:Double x:Key="ThemePageMaxWidth">1000</x:Double>
    <x:Double x:Key="ThemeContentMaxWidth">800</x:Double>
    <x:Double x:Key="ThemeContentWideMinWidth">600</x:Double>
    <x:Double x:Key="ThemeContentWideMaxWidth">960</x:Double>
    <x:Double x:Key="ThemeSectionSpacing">24</x:Double>

    <Thickness x:Key="ThemeButtonPaddingSm">8,4</Thickness>
    <Thickness x:Key="ThemeButtonPaddingMd">12,6</Thickness>
    <Thickness x:Key="ThemeButtonPaddingLg">16,10</Thickness>
    <x:Double x:Key="ThemeButtonMinHeight">32</x:Double>
    <x:Double x:Key="ThemeButtonMinHeightLg">40</x:Double>

    <Thickness x:Key="ThemeInputPadding">12,8</Thickness>
    <x:Double x:Key="ThemeInputMinHeight">36</x:Double>
    <x:Double x:Key="ThemeSearchBoxHeight">36</x:Double>

    <FontFamily x:Key="ThemeFontSans">Inter, Segoe UI, -apple-system, Noto Sans, sans-serif, Segoe UI Emoji, Noto Color Emoji</FontFamily>
    <FontFamily x:Key="ThemeFontMono">avares://PrivStack.Desktop/Assets/Fonts#JetBrains Mono, Consolas, Menlo, monospace</FontFamily>

    <x:Double x:Key="ThemeFontSize2Xs">10</x:Double>
    <x:Double x:Key="ThemeFontSizeXs">11</x:Double>
    <x:Double x:Key="ThemeFontSizeXsSm">12</x:Double>
    <x:Double x:Key="ThemeFontSizeSm">13</x:Double>
    <x:Double x:Key="ThemeFontSizeSmMd">14</x:Double>
    <x:Double x:Key="ThemeFontSizeMd">15</x:Double>
    <x:Double x:Key="ThemeFontSizeLg">17</x:Double>
    <x:Double x:Key="ThemeFontSizeLgXl">20</x:Double>
    <x:Double x:Key="ThemeFontSizeXl">22</x:Double>
    <x:Double x:Key="ThemeFontSize2Xl">26</x:Double>
    <x:Double x:Key="ThemeFontSize3Xl">34</x:Double>
    <x:Double x:Key="ThemeFontSize4Xl">48</x:Double>
    <!-- Line Heights -->
    <x:Double x:Key="ThemeLineHeightXs">14</x:Double>
    <x:Double x:Key="ThemeLineHeightSm">22</x:Double>
    <x:Double x:Key="ThemeLineHeightMd">24</x:Double>
    <x:Double x:Key="ThemeLineHeightBase">26</x:Double>
    <x:Double x:Key="ThemeLineHeightLg">30</x:Double>
    <x:Double x:Key="ThemeLineHeightXl">44</x:Double>

</ResourceDictionary>
